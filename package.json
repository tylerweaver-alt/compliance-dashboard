{
  "name": "compliance-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:webpack": "next dev --webpack",
    "build": "next build",
    "build:webpack": "next build --webpack",
    "start": "next start",
    "start:webpack": "next start --webpack",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "fetch:nws": "tsx scripts/fetchNwsAlerts.ts",
    "audit:sql": "tsx scripts/security/audit-sql.ts",
    "verify:security": "tsx scripts/security/verify-all.ts",
    "test": "echo \"No tests configured yet - see CONTRIBUTING.md\" && exit 0",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": ["eslint --fix", "prettier --write"],
    "*.{json,md}": ["prettier --write"]
  },
  "dependencies": {
    "@sentry/nextjs": "^10.29.0",
    "@sentry/react": "^10.29.0",
    "@turf/turf": "^7.3.1",
    "@vercel/blob": "^2.0.0",
    "@vercel/functions": "^3.3.4",
    "csv-parse": "^6.1.0",
    "leaflet": "^1.9.4",
    "leaflet.heat": "^0.2.0",
    "next": "^16.0.7",
    "next-auth": "^4.24.13",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-leaflet": "^5.0.0",
    "recharts": "^3.5.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/geojson": "^7946.0.16",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^20",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.2",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "typescript": "^5"
  }
